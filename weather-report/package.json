{
  "name": "weather-checker-app",
  "version": "1.0.0",
  "description": "Weather checker backend application with intentional vulnerabilities",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node source/app.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint source/**/*.ts --ext .ts",
    "lint:fix": "eslint source/**/*.ts --ext .ts --fix",
    "format": "prettier --write source/**/*.ts",
    "format:check": "prettier --check source/**/*.ts"
  },
  "keywords": [
    "weather",
    "api",
    "vulnerable",
    "practice"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.18.0",
    "body-parser": "1.18.2",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "express": "4.16.0",
    "morgan": "1.9.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.4",
    "@types/dotenv": "^8.2.3",
    "@types/express": "4.16.0",
    "@types/jest": "^29.5.0",
    "@types/morgan": "1.7.35",
    "@types/node": "10.12.0",
    "@types/node-fetch": "^2.6.12",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
